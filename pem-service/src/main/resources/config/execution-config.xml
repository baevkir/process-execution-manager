<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="executionProcessService" class="com.pem.logic.service.process.impl.ExecutionProcessServiceImpl">
        <property name="converterFactory" ref="converterFactory"/>
        <property name="persistenceService" ref="processPersistenceService"/>
    </bean>

    <bean id="operationExecutor" class="com.pem.logic.service.executor.impl.OperationExecutorImpl">
        <property name="contextProvider" ref="operationContextProvider"/>
        <property name="converterFactory" ref="converterFactory"/>
        <property name="processService" ref="executionProcessService"/>
    </bean>
</beans>